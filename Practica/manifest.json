{
  "manifest_version": 3,
  "name": "NetTime",
  "version": "1.0",
  "description": "MonitorizeazÄƒ timpul petrecut pe site-uri.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
